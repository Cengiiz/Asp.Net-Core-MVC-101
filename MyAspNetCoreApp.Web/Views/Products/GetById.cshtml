
@{
    ViewData["Title"] = "GetById";
}
@model ProductViewModel

<a class="btn btn-primary mb-2" asp-controller="products" asp-action="index">Back</a>
<div class="card">
    <div class="card-header">
        Urun(@Model.Name) 
    </div>
    <div class="card-body">
        <div>Id: @Model.Id</div>
        <div>Description: @Model.Description</div>
        <div>Price: @Model.Price</div>
        <div>Stock: @Model.Stock</div>
        <div>Color: @Model.Color</div>
        <div>Expire: @Model.Expire</div>
    </div>
</div>

